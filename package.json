{
  "name": "rougish",
  "version": "0.1.0",
  "description": "Simple tile based rouglike",
  "main": "dist/index.js",
  "type": "module",
  "engines": {
    "node": ">= 16"
  },
  "engineStrict": true,
  "npmClient": "pnpm",
  "scripts": {
    "start": "cross-env NODE_ENV=development DEVTOOL=eval-source-map webpack serve",
    "clean": "rimraf dist",
    "build": "webpack",
    "build:dev": "cross-env NODE_ENV=development DEVTOOL=eval-source-map webpack",
    "format": "prettier -c --write \"**/*.{css,html,cjs,mjs,js,json,ts}\"",
    "format:check": "prettier --list-different \"**/*.{css,html,cjs,mjs,js,json,ts}\"",
    "lint": "eslint .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ashnel3/rougish.github.io.git"
  },
  "keywords": [
    "rouge",
    "rougelike"
  ],
  "author": "ashnel3",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ashnel3/rougish.github.io/issues"
  },
  "homepage": "https://github.com/ashnel3/rougish.github.io#readme",
  "devDependencies": {
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "esbuild-loader": "^3.0.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.1",
    "json-minimizer-webpack-plugin": "^4.0.0",
    "mini-css-extract-plugin": "^2.7.6",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "typescript": "^5.0.4",
    "webpack": "^5.84.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0"
  }
}
